<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MauiAppApi.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MauiAppApi"
    Shell.FlyoutBehavior="Flyout"
    Title="MauiAppApi">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    <ShellContent
        Title="Kurlar"
        ContentTemplate="{DataTemplate local:KurlarPage}"
        Route="KurlarPage" />

    <ShellContent
        Title="Hava Durumu"
        ContentTemplate="{DataTemplate local:HavaDurumuPage}"
        Route="HavaDurumuPage" />

</Shell>
